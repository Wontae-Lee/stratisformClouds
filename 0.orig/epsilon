FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      epsilon;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -3 0 0 0 0];

internalField   uniform 0.01;

boundaryField
{
    "(warmPlace1|warmPlace2|coldPlace)"
    {
        type            epsilonWallFunction;
        value           $internalField;
    }
    "(inlet|outlet|allBoundaries|atmosphere)"
    {
        type            fixedValue;
        value           $internalField;
    }

}


// ************************************************************************* //
